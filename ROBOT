#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    
    while(t--)
    {
        string s;
        cin>>s;
        bool flag=0;
        int x=0,y=0;
        for(int i=0; i<s.size(); i++)
        {
             if(s[i]=='L')
             {
                 x--;
                 
             }
             else if(s[i]=='R')
             {
                 x++;
             }
             else if(s[i]=='U')
             {
                 y++;
             }
             else
             {
                 y--;
             }
             int xx=0,yy=0;
             for(int j=0; j<s.size(); j++)
             {
                 if(s[j]=='L')
             {
                 xx--;
                 
             }
             else if(s[j]=='R')
             {
                 xx++;
             }
             else if(s[j]=='U')
             {
                 yy++;
             }
             else
             {
                 yy--;
             }
             if(x==xx && y==yy)
             {
                  if(s[j]=='L')
             {
                 xx++;
                 
             }
             else if(s[j]=='R')
             {
                 xx--;
             }
             else if(s[j]=='U')
             {
                 yy--;
             }
             else
             {
                 yy++;
             }
             }
             
             }
             if(xx==0 && yy==0)
             {
                 flag=1;
                 cout<<x<<" "<<y<<endl;
                 break;
             }
        }
        if(flag==0)
        {
            cout<<0<<" "<< 0<<endl;
        }
    }
}
